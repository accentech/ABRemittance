      <parameter>
        <parameterName value="@Who" />
        <dbType value="Int32" />
        <layout type="log4net.Layout.PatternLayout">
          <conversionPattern value="%property{Who}" />
        </layout>
      </parameter>
      <parameter>
        <parameterName value="@When" />
        <dbType value="DateTime" />
        <layout type="log4net.Layout.RawTimeStampLayout" />
      </parameter>
      <parameter>
