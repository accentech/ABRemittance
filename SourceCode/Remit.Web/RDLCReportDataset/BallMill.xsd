                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>Select se.Date, fi.BallMill From 
(
	  Select cast(Date as date) as Date, Sum([DailyBallMill]) as BallMill FROM ItemIssue
	 Group By cast(Date as date) 
) 
as fi RIGHT Join 
(
	SELECT Date = DATEADD(Day,Number,@startDate) 
	FROM  master..spt_values 
	WHERE Type='P'
	AND DATEADD(day,Number,@startDate) &lt; @endDate
) 
	as se on fi.Date = se.Date</CommandText>
