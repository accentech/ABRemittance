<body >
    <div class="navbar navbar-fixed-top" style="border-bottom: 5px solid #390f39;">
        <nav class="" style="margin-bottom: 0 !important; padding-bottom: 0 !important; background-color: #fff !important; border-style: none !important">
            <div class="container-fluid">
                <!-- Brand and toggle get grouped for better mobile display -->
                <div class="navbar-header" style="padding:0; margin:0 15px 0 0;">
                    <a href="/Account/Index" class="navbar-brand" style="padding:0; margin:0"><img src="../../Content/Images/logo.png" alt="Shop Floor Control" height="60" /></a>
                </div>

                <!-- Collect the nav links, forms, and other content for toggling -->
                <!-- /.navbar-collapse -->
            </div><!-- /.container-fluid -->
        </nav>
    </div>
    <div class="container-fluid at-container" style="padding-top: 70px;">
        @RenderBody()
    </div>

    <div class="navbar navbar-default navbar-fixed-bottom at-navbar-footer" style="background-color: #F5F5F5; height: 63px !important; ">
        <div class="container" style="height: 28px; font-size: 10px">
        </div>
        <div class="row" style="background-color: #390F39; height: 35px">
            <p class="navbar-text" style=" color:white !important ">&copy; @DateTime.UtcNow.Year - Developed by AccenTech Limited</p>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("Scripts", required: false)
</body>
</html>
