<script type="text/javascript">
    app.directive('fileModel', ['$parse',
        function ($parse) {
            return {
                restrict: 'A',
                link: function (scope, element, attrs) {
                    var model = $parse(attrs.fileModel);
                    var modelSetter = model.assign;
                    element.bind('change', function () {
                        scope.$apply(function () {
                            modelSetter(scope, element[0].files[0]);
                        });
                    });
                }
            };
        }
    ]);
    app.filter('startFrom', function () {
        return function (input, start) {
            if (input) {
                start = +start; //parse to int
                return input.slice(start);
            }
            return [];
        }
    });
    app.factory('focus', function ($rootScope, $timeout) {
        return function (name) {
            $timeout(function () {
                $rootScope.$broadcast('focusOn', name);
            });
        };
    });
    app.factory('zoneCommisionService', ['$http', function ($http) {
        return {
            saveZoneCommision: function (zoneCommision) {
                var params = { zoneCommision: zoneCommision }
                return $http({
                    url: '/ZoneCommision/CreateZoneCommision',
                    method: 'POST',
                    type: "json",
                    data: JSON.stringify(params)
                });
            },
            updateFGItemIn: function (zoneCommision, fgItemInList) {
                var params = { itemIn: zoneCommision, fgItemInDetails: fgItemInList, status: status }
                return $http({
                    url: '/ZoneCommision/UpdateFGItemIn',
                    method: 'POST',
                    type: "json",
                    data: JSON.stringify(params)

                });
            },

            getFGItemInListByYearMonth: function (year, month) {
                return $http.get('/ZoneCommision/GetReceiveListByYear', { params: { year: year, month: month + 1 } });
            },
            getAllDivision: function () {
                return $http.get('/Division/GetDivisionList');
            },

            deleteZoneCommision: function (id) {
                return $http.get('/ZoneCommision/DeleteZoneCommision?id=' + id);
            },
            getAllDealerZone: function () {
                return $http.get('/FGDealerZone/GetFGDealerZoneListWithoutOther');
            },
            getDateWhenUpdate: function (zoneId) {
                return $http.get('/ZoneCommision/GetEffectiveDate?zoneId=' + zoneId);
            },
            getAllData: function (data) {
                //var params = { getAllData: data }
                //return $http({
                //    url: '/ZoneCommision/GetZoneCommisionByZoneAndDate',
                //    method: 'POST',
                //    type: "json",
                //    data: JSON.stringify(params)
                //});
                return $http.get('/ZoneCommision/GetZoneCommisionByZoneAndDate?id=' + data);
            }
        };
    }]);

    app.controller('zoneCommisionCtrl',
        function ($scope, $timeout, $http, focus, $location, $anchorScroll, zoneCommisionService, $log, $ngBootbox, $filter) {
            $scope.selectedYear = new Date();
            $scope.EffectiveDate = new Date().toISOString();
            $scope.editmode = false;
            $scope.notApprovemode = true;
            @*$scope.ReceivedBy = @UserSession.GetUserFromSession().Employee.Id*@
            //$scope.zoneDetailsSFT = [];
            //$scope.zoneDetailsPCS = [];
            //$scope.zoneDetailsCTN = [];
            //$scope.zoneDetailsSMT = [];

            $scope.messageModalObj = {};
            $scope.messageModalObj.message = '';
            $scope.showModalforSearch = false;
            $scope.selectedRow = null;

            $scope.zoneDetailsSFT = [];
            $scope.zoneDetailsSFT.push({
                Id: '',
                FGUOMId: 3,
                MonthlyTarget: 0,
                MonthlyCommission: 0,
                QuarterlyCommission: 0,
                HalfYearlyCommission: 0,
                YearlyCommission: 0
            });
            $scope.addNewDetailSFT = function ($event) {
                $scope.zoneDetailsSFT.push({
                    Id: '',
                    FGUOMId: 3,
                    MonthlyTarget: 0,
                    MonthlyCommission: 0,
                    QuarterlyCommission: 0,
                    HalfYearlyCommission: 0,
                    YearlyCommission: 0
                });
            }

            $scope.zoneDetailsPCS = [];
            $scope.zoneDetailsPCS.push({
                Id: '',
                FGUOMId: 2,
                MonthlyTarget: 0,
                MonthlyCommission: 0,
                QuarterlyCommission: 0,
                HalfYearlyCommission: 0,
