                    }

                    if ($scope.zoneDetailsPCS != null || $scope.zoneDetailsPCS !== "") {
                        if ($scope.zoneDetailsPCS.length > 0) {
                            for (var j = 0; j < $scope.zoneDetailsPCS.length; j++) {
