<script type="text/javascript">
    app.filter('startFrom', function () {
        return function (input, start) {
            if (input) {
                start = +start; //parse to int
                return input.slice(start);
            }
            return [];
        }
    });
    app.factory('focus', function ($rootScope, $timeout) {
        return function (name) {
            $timeout(function () {
                $rootScope.$broadcast('focusOn', name);
            });
        };
    });
    app.factory('autoGeneratedNoService', ['$http', function ($http) {

        return {
            saveAutoGeneratedNo: function (autoGeneratedNo) {
                return $http({
                    url: '/AutoGeneratedNo/CreateAutoGeneratedNo',
                    method: 'POST',
                    data: autoGeneratedNo
                });
            },
            getAutoGeneratedNo: function (id) {
                return $http.get('/AutoGeneratedNo/GetAutoGeneratedNo/' + id);
            },
            getAllAutoGeneratedNo: function () {
                return $http.get('/AutoGeneratedNo/GetAutoGeneratedNoList');
            },
            deleteAutoGeneratedNo: function (autoGeneratedNo) {
                return $http({
                    url: '/AutoGeneratedNo/DeleteAutoGeneratedNo',
                    method: 'POST',
                    data: autoGeneratedNo
                });
            }
        };
    }]); app.controller('autoGeneratedNo', function ($scope, $timeout, $http, focus, $location, $anchorScroll, autoGeneratedNoService, $log, $ngBootbox) {
        if ('@ViewBag.permission' == "NO") {
            $scope.NoPermission = true;
        }
        $scope.a = false;
        $scope.StartIndex = 1;
        $scope.LastIndex = 0;
        $scope.messageModalObj = {};
        $scope.messageModalObj.message = '';
        loadAllAutoGeneratedNo();
        $scope.showModalforSearch = false;
        // Add a new autoGeneratedNo
        $scope.addAutoGeneratedNo = function () {
            $scope.$broadcast('show-errors-check-validity');
            if ($scope.form.$valid) {
                var autoGeneratedNo = {};
                autoGeneratedNo["Id"] = $scope.Id;
                autoGeneratedNo["Type"] = $scope.Type;
                autoGeneratedNo["Prefix"] = $scope.Prefix;
                autoGeneratedNo["PeriodFormat"] = $scope.PeriodFormat;
                autoGeneratedNo["RefreshFrequency"] = $scope.RefreshFrequency;
                autoGeneratedNo["Format"] = $scope.Format;
                autoGeneratedNo["StartIndex"] = $scope.StartIndex;
                autoGeneratedNo["LastIndex"] = $scope.LastIndex;
                autoGeneratedNo["LastIndexDate"] = $scope.LastIndexDate;
                autoGeneratedNo["Separator1"] = $scope.Separator1;
                autoGeneratedNo["Separator2"] = $scope.Separator2;

                autoGeneratedNoService.saveAutoGeneratedNo(autoGeneratedNo)
                .then(function onSuccess(response) {
                    loadAllAutoGeneratedNo();
                    $scope.messageModalObj.message = response.data.message;
                    $scope.showModalforSearch = true;
                    $timeout(function () { $scope.showModalforSearch = false; }, 3000);
                    $scope.reset();

                })
                .catch(function onError(response) {
                    $ngBootbox.alert('@Resources.ResourceCommon.MsgErrorInSaving');
                });
            }
        };
        // Populate autoGeneratedNo

        $scope.selectedRow = null;
        $scope.populateAutoGeneratedNo = function (rowvalue) {
            $scope.a = true;
            $scope.selectedRow = rowvalue;
            $scope.Id = rowvalue.Id;
            $scope.Type = rowvalue.Type;
            $scope.Prefix = rowvalue.Prefix;
            $scope.PeriodFormat = rowvalue.PeriodFormat;
            $scope.RefreshFrequency = rowvalue.RefreshFrequency;
            $scope.Format = rowvalue.Format;
            $scope.StartIndex = rowvalue.StartIndex;
            $scope.LastIndex = rowvalue.LastIndex;
            $scope.LastIndexDate = rowvalue.LastIndexDate;
            $scope.Separator1 = rowvalue.Separator1;
            $scope.Separator2 = rowvalue.Separator2;
            $location.hash('searchDivId');
            $anchorScroll();
            focus('focusMe');
            $scope.desableIdField = true;
        }

        $scope.reset = function () {
            $scope.$broadcast('show-errors-reset');
            $scope.selectedRow = null;
            $scope.a = false;
            $scope.Id = '';
            $scope.Type =null;
            $scope.Prefix = '';
            $scope.PeriodFormat = '';
            $scope.RefreshFrequency = '';
            $scope.Format = '';
            $scope.StartIndex = '';
            $scope.LastIndex = '';
            $scope.Separator1 = '';
            $scope.Separator2 = '';

        }

        $scope.deleteAutoGeneratedNo = function (autoGeneratedNo) {
            autoGeneratedNoService.deleteAutoGeneratedNo(autoGeneratedNo)
            .then(function onSuccess(response) {
                $scope.messageModalObj.message = response.data.message;
                $scope.showModalforSearch = true;
                $timeout(function () { $scope.showModalforSearch = false; }, 3000);
                $scope.reset();
                loadAllAutoGeneratedNo();
            })
            .catch(function onError(response) {
                $ngBootbox.alert('@Resources.ResourceCommon.MsgNot_deleted');
            });
        }
        // Load employee
        function loadAllAutoGeneratedNo() {
            $scope.allAutoGeneratedNoFromDb = [];
            autoGeneratedNoService.getAllAutoGeneratedNo()
                    .then(function onSuccess(response) {
                        $scope.allAutoGeneratedNoFromDb = response.data;
                    })
                    .catch(function onError(response) {
                        $ngBootbox.alert('@Resources.ResourceCommon.MsgErrorInLoading');
                    });
        }

        $scope.formatId = function () {
            $scope.Format = "";
            for (i = 0; i < $scope.FormatValue; i++) {
                $scope.Format += '#';

            }
        }

        $scope.newArray = [
        ];

        $scope.tp = '@WebConfigurationManager.AppSettings["Type"]';
        $scope.newType = $scope.tp.split(",");

        for (var i = 0; i < $scope.newType.length; i++) {
            $scope.newArray.push({
                type: $scope.newType[i],
                name: $scope.newType[i]
            });
        }

        $scope.currentPage = 1; //current page
        $scope.entryLimit = 10; //max no of items to display in a page
        $scope.filteredItems = $scope.allAutoGeneratedNoFromDb.length; //Initially for no filter
        $scope.totalItems = $scope.allAutoGeneratedNoFromDb.length;
        //$scope.maxSize = 15;

        $scope.setPage = function (pageNo) {
            $scope.currentPage = pageNo;
        };
        $scope.filter = function () {
            $timeout(function () {
                $scope.filteredItems = $scope.filtered.length;
            }, 9000);
        };
        $scope.sort_by = function (predicate) {
            $scope.predicate = predicate;
            $scope.reverse = !$scope.reverse;
        };
    });

</script>
